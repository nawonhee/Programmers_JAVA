class Solution {
    public String solution(int age) {
        String answer = "";
        String age_s = Integer.toString(age);
        
        for(int i=0;i<age_s.length();i++){
            int n = (int) age_s.charAt(i);
            char c = (char) (n + 49);
            answer += String.valueOf(c);
        }
        
        return answer;
    }
}
